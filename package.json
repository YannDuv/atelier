{
  "name": "@yannduval/atelier",
  "description": "Atelier is a web component library that use simplicity for fast building applications.",
  "version": "0.1.0-alpha",
  "private": false,
  "keywords": [
    "lit-element",
    "web-components",
    "design-system"
  ],
  "customElements": "custom-elements.json",
  "author": "Yann Duval <yann.duval@gmx.fr>",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -s public -p 6006",
    "analyze": "npx web-component-analyzer 'src/**/*.{js,ts}' --outFile custom-elements.json",
    "prepare": "npm run analyze"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": "./dist/*"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@storybook/addon-actions": "6.3.0-alpha.35",
    "@storybook/addon-essentials": "6.3.0-alpha.35",
    "@storybook/addon-links": "6.3.0-alpha.35",
    "@storybook/web-components": "6.3.0-alpha.35",
    "babel-loader": "8.2.2",
    "custom-elements-manifest": "0.1.0",
    "lit-html": "^1.4.1",
    "storybook": "6.2.9",
    "ts-lit-plugin": "1.2.1",
    "typescript": "4.1.3",
    "vite": "2.2.3"
  },
  "dependencies": {
    "lit": "^2.0.0-rc.1",
    "lit-html-dev": "npm:lit-html@2.0.0-rc.2"
  }
}
